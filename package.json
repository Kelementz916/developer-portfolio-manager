{
  "name": "developer-portfolio-manager",
  "version": "1.0.0",
  "sideEffects": false,
  "description": "A portfolio manager for developers",
  "main": "backend/src/server.js",
  "scripts": {
    "clean": "rimraf dist/",
    "start": "nodemon backend/src/server.js",
    "start:dev": "concurrently \"npm run start:front\" \"npm run start:back\"",
    "start:front": "webpack serve --config webpack.config.js --mode development --open",
    "start:back": "nodemon backend/src/server.js",
    "build": "webpack --config frontend/webpack.config.js --mode production",
    "build-dev": "webpack --config frontend/webpack.config.js --mode development",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "mongoose": "^8.1.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-react": "^7.23.3",
    "babel-loader": "^9.1.3",
    "concurrently": "^8.2.2",
    "css-loader": "^6.10.0",
    "css-minimizer-webpack-plugin": "^6.0.0",
    "html-webpack-plugin": "^5.6.0",
    "mini-css-extract-plugin": "^2.8.0",
    "nodemon": "^3.0.3",
    "sass": "^1.54.5",
    "sass-loader": "^14.1.0",
    "style-loader": "^3.3.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.90.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.1"
  }
}
